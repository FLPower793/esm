
@{
    ViewBag.Title = "Status";  
}

<script src="~/Scripts/jquery-1.10.2.js"></script>
<script src="~/Scripts/jquery.signalR-2.2.1.js"></script>
<script src="/signalr/hubs"></script>
<script src="~/Scripts/jquery.cookie.js"></script>
<script src="~/Scripts/client_main_v1.5.js"></script>

<h2>Статус</h2>
<table border="1">
    <tr><th>Пользователь</th><th>В сети?</th><th>Решает задачу</th><th>Номер задачи</th><th>Дата</th></tr>
    @foreach (var n in ViewData["UserStat"] as List<String[]>)
    {
        <tr><td>@n[1]</td><td>@n[5]</td><td>@n[2]</td><td>@n[3]</td><td>@n[4]</td><td><input type="button" value="Сброс задачи" onclick="location.href='@Url.Action("resetTask", "Home", new { id = Convert.ToInt32(@n[0]) })'"/></td></tr>
    }
</table>
<a href="~/Home/Master">На главную</a>
